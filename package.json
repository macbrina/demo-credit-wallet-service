{
  "name": "demo-credit-wallet-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "knex:migrate": "ts-node ./node_modules/knex/bin/cli.js migrate:latest --knexfile=knexfile.ts",
    "knex:migrate:rollback": "ts-node ./node_modules/knex/bin/cli.js migrate:rollback --knexfile=knexfile.ts",
    "knex:seed": "ts-node ./node_modules/knex/bin/cli.js seed:run --knexfile=knexfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "nodemonConfig": {
    "watch": [
      "src",
      "knexfile.ts",
      "db.ts"
    ],
    "exec": "tsc && node ./dist/src/index.js",
    "ext": "ts,js,json"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.8.4",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.7",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql2": "^3.11.3",
    "winston": "^3.15.0"
  },
  "_moduleAliases": {
    "@": "./dist/src"
  }
}
